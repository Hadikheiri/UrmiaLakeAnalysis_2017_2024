ion/Random forest/Sieve/2024.tif'  # Update this with your file path\n","\n","classified_2017, transform, crs = load_classified_image(classified_2017_path)\n","classified_2024, _, _ = load_classified_image(classified_2024_path)\n","\n","# Define class values\n","NON_WATER_SALT_CLASS = 1  # Non-Water-Salt\n","WATER_CLASS = 2           # Water\n","SALT_CLASS = 3            # Salt\n","\n","# Create an empty change detection map to store all 9 change types\n","change_map = np.zeros_like(classified_2017, dtype=np.int8)\n","\n","# Map changes from 2017 to 2024\n","change_types = {\n","    (NON_WATER_SALT_CLASS, NON_WATER_SALT_CLASS): 1,\n","    (NON_WATER_SALT_CLASS, WATER_CLASS): 2,\n","    (NON_WATER_SALT_CLASS, SALT_CLASS): 3,\n","    (WATER_CLASS, NON_WATER_SALT_CLASS): 4,\n","    (WATER_CLASS, WATER_CLASS): 5,\n","    (WATER_CLASS, SALT_CLASS): 6,\n","    (SALT_CLASS, NON_WATER_SALT_CLASS): 7,\n","    (SALT_CLASS, WATER_CLASS): 8,\n","    (SALT_CLASS, SALT_CLASS): 9\n","}\n","\n","# Fill change_map with change type values\n","for (class_2017, class_2024), change_value in change_types.items():\n","    change_map[(classified_2017 == class_2017) & (classified_2024 == class_2024)] = change_value\n","\n","# Calculate statistics of changes\n","change_counts = {change_value: np.sum(change_map == change_value) for change_value in change_types.values()}\n","\n","# Print statistics\n","for change_type, count in change_counts.items():\n","    print(f\"Change type {change_type} (Class {list(change_types.keys())[change_type-1]}): {count} pixels\")\n","\n","# Save the change detection result\n","change_detection_output_path = '/content/drive/MyDrive/Urmia/Change Detectiom/Change_Detection_Result.tif'  # Update this with your desired path\n","with rasterio.open(\n","    change_detection_output_path,\n","    'w',\n","    driver='GTiff',\n","    height=change_map.shape[0],\n","    width=change_map.shape[1],\n","    count=1,\n","    dtype=change_map.dtype,\n","    crs=crs,\n","    transform=transform\n",") as dst:\n","    dst.write(change_map, 1)\n","\n","print(f\"Change detection result saved to {change_detection_output_path}\")\n","\n","# Define a custom colormap with distinct colors for each change type\n","colors = ['#d3d3d3',  # Change 1: Non-Water-Salt to Non-Water-Salt (light gray)\n","          '#0000ff',  # Change 2: Non-Water-Salt to Water (blue)\n","          '#ff8c00',  # Change 3: Non-Water-Salt to Salt (orange)\n","          '#8b0000',  # Change 4: Water to Non-Water-Salt (dark red)\n","          '#1e90ff',  # Change 5: Water to Water (light blue)\n","          '#ffff00',  # Change 6: Water to Salt (yellow)\n","          '#ff1493',  # Change 7: Salt to Non-Water-Salt (pink)\n","          '#00ff00',  # Change 8: Salt to Water (green)\n","          '#800080']  # Change 9: Salt to Salt (purple)\n","\n","cmap = ListedColormap(colors)\n","\n","# Plot the change detection result\n","plt.figure(figsize=(10, 6))\n","plt.title('Change Detection Map')\n","im = plt.imshow(change_map, cmap=cmap, interpolation='none')\n","#plt.colorbar(im, ticks=np.arange(1, 10), label='Change Type')\n","\n","# Add legend for class changes\n","legend_labels = [\n","    'Non-Water-Salt to Non-Water-Salt',\n","    'Non-Water-Salt to Water',\n","    'Non-Water-Salt to Salt',\n","    'Water to Non-Water-Salt',\n","    'Water to Water',\n","    'Water to Salt',\n","    'Salt to Non-Water-Salt',\n","    'Salt to Water',\n","    'Salt to Salt'\n","]\n","plt.legend(handles=[plt.Rectangle((0, 0), 1, 1, color=cmap(i)) for i in range(9)], labels=legend_labels, bbox_to_anchor=(1.05, 1), loc='upper left')\n","\n","plt.show()\n"]},{"cell_type":"code","source":["!pip install rasterio\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"NpaZfZ3HPPVT","executionInfo":{"status":"ok","timestamp":1724463271186,"user_tz":-120,"elapsed":6701,"user":{"displayName":"Hadi Kheiri","userId":"14650816525708423471"}},"outputId":"fc57fe96-2a01-4d1a-da5f-684482beb9fa"},"execu